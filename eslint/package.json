{
    "name": "@shaunburdick/eslint-config",
    "version": "1.0.0",
    "description": "Shaun's style configuration",
    "main": "index.js",
    "type": "module",
    "files": [
        "es6",
        "typescript",
        "index.js"
    ],
    "scripts": {
        "fix": "npm run lint:fix",
        "lint:fix": "eslint . --fix",
        "lint": "eslint .",
        "test": "npm run lint",
        "version": "chg release -y && git add -A CHANGELOG.md"
    },
    "keywords": [
        "typescript",
        "lint",
        "eslint",
        "eslintconfig"
    ],
    "author": "Shaun Burdick <npm@shaunburdick.com>",
    "license": "UNLICENSED",
    "engines": {
        "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
    },
    "peerDependencies": {
        "eslint": ">=9"
    },
    "peerDependenciesMeta": {
        "typescript": {
            "optional": true
        }
    },
    "dependencies": {
        "@stylistic/eslint-plugin": "^2.6.2",
        "eslint-plugin-jsdoc": "^50.0.0",
        "eslint-plugin-security": "^3.0.1",
        "typescript-eslint": "^8.0.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@types/eslint__js": "^8.42.3",
        "@types/node": "^22.2.0",
        "chg": "^0.4.0",
        "eslint": "^9.8.0",
        "typescript": "^5.5.4"
    }
}
